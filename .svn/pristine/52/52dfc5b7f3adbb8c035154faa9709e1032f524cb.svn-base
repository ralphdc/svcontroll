								<th class="center"><h1>备注</h1></th>
							</thead>
							<volist id="vo" name="sdetail">
							<tr>
								<td>{$vo['element']}</td>
								<td>{$vo['ip']}</td>
								<td>{$vo['info']}</td>
								<td>{$vo['time']}</td>
								<td>{$vo['remark']}</td>
							</tr>
						</volist>
						</tbody>
					</table>
			</div>
			<div class="tabsFooter">
				<div class="tabsFooterContent"></div>
			</div>
