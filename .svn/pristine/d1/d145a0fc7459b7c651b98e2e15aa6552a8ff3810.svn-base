/* button */
a.btnAdd, a.btnDel, a.btnView, a.btnEdit, a.btnSelect, a.btnUnSelect, a.btnInfo, a.btnAssign, a.btnLook,a.btnWechat,a.btnFollow,a.btnFollow, a.btnAttach{background:url(../img/button/iconbg.png) no-repeat; display:block; width:22px; height:20px; text-indent:-1000px; overflow:hidden; float:left; margin-left: 6px}
table .hover a.btnAdd, table .hover a.btnDel, table .hover a.btnView, table .hover a.btnEdit, table .hover a.btnSelect, table .hover a.btnUnSelect, table .hover a.btnInfo, table .hover a.btnAssign, table .hover a.btnLook,.hover a.btnWechat,.hover a.btnFollow, table .hover a.btnAttach, a.btnLook:hover {background:url(../img/button/iconbg2.png) }
a.btnAdd, table .hover a.btnAdd {background-position:0 0}
a.btnDel, table .hover a.btnDel {background-position: -23px 0}
a.btnInfo, table .hover a.btnInfo {background-position: -46px 0}
a.btnAssign, table .hover a.btnAssign {background-position: -69px 0}
a.btnView, table .hover a.btnView {background-position: -115px 0}
a.btnEdit, table .hover a.btnEdit {background-position: -138px 0}
a.btnWechat, table .hover a.btnWechat {background-position: -233px 0}
a.btnFollow, table .hover a.btnFollow {background-position: -264px 0}
a.btnSelect, table .hover a.btnSelect {background-position: -207px 0}
a.btnUnSelect, table .hover a.btnUnSelect {background-position: -92px 0}
a.btnLook, table .hover a.btnLook, a.btnLook:hover {background-position: -161px 0; margin-left:1px}
a.btnWechat:hover {background-position: -238px 0; margin-left:1px}
a.btnWechat:hover {background-position: -248px 0; margin-left:1px}

a.btnAttach, table .hover a.btnAttach {background-position: -183px 0}
.center a.btnAdd, .center a.btnDel, .center a.btnView, .center a.btnEdit, .center a.btnSelect, .center a.btnInfo, 
.center a.btnAssign, .center a.btnLook, .center a.btnAttach {display:inline-block;float:none; vertical-align:top}
.right a.btnAdd, .right a.btnDel, .right a.btnView, .right a.btnEdit, .right a.btnSelect, .right a.btnInfo, 
