#header .themeList li.default div { background-position:0 0;}
#header .themeList li.default .selected { background-position:0 -20px;}
#header .themeList li.green div { background-position:-20px 0;}
#header .themeList li.green .selected { background-position:-20px -20px;}
#header .themeList li.red div { background-position:-40px 0;}
#header .themeList li.red .selected { background-position:-40px -20px;}
#header .themeList li.purple div { background-position:-60px 0;}
#header .themeList li.purple .selected { background-position:-60px -20px;}
#header .themeList li.silver div { background-position:-80px 0;}
#header .themeList li.silver .selected { background-position:-80px -20px;}
#header .themeList li.azure div { background-position:-100px 0;}
#header .themeList li.azure .selected { background-position:-100px -20px;}

.toggleCollapse { display:block; overflow:hidden; height:32px; background-repeat:repeat-x;}
.toggleCollapse h2 { float:left; padding-left:48px; line-height:32px;}
.toggleCollapse div { float:right; display:block; overflow:hidden; width:25px; height:25px; text-indent:-100px; background-position:100% -47px; cursor:pointer;}
.toggleCollapse div:hover { background-position:100% -97px;}

#sidebar_s .collapse { height:430px; cursor:pointer;}
#sidebar_s .toggleCollapse { border:0; background:none;}
#sidebar_s .toggleCollapse div { width:21px; border:0; background-position:-2px -147px;}
#sidebar_s .toggleCollapse div:hover { background-position:-2px -197px;}

/* Menu */
#navMenu { display:block; overflow:hidden; height:35px; padding:0 5px; background-repeat:repeat-x;}
#navMenu ul { float:left; display:block; overflow:hidden; position:relative;}
#navMenu li { float:left; margin-left:-1px; background-position:0 -50px; position:relative;}
#navMenu li a { float:left;}
#navMenu li span { float:left; font-weight:bold; cursor:pointer; padding:0 15px;}
#navMenu li a, #navMenu li span { line-height:35px; font-size:14px;}
#navMenu li.selected { z-index:1;}
#navMenu li.selected a, #navMenu li.selected a:hover { text-decoration:none; background-position:100% -150px;}
#navMenu li.selected span { padding:0 8px; background-position:0 -100px;}

/* Pages default */
.page { display:block; overflow:hidden;float:left; width:100%;}
.pageHeader { display:block; overflow:auto; margin:1px 0; padding:6px; position:relative;}
.searchBar { overflow:hidden}
.searchBar ul.searchContent { display:block; overflow:hidden; float:left; padding: 5px 0;}
.searchBar ul.searchContent li { float:left; display:block; overflow:hidden; padding:0; margin-right:20px}
.searchBar label { float:left; padding:0 5px; line-height:23px; color:#616161}
.searchBar .searchContent td { padding-right:20px; white-space:nowrap; height:25px}
.searchBar table.searchContent { border-collapse: collapse;}
.searchBar .subBar { height:25px; float:left; margin-top:2px}
.searchBar .subBar ul { float:right;}
.searchBar .subBar li { float:left; margin-left:5px;}
.pageContent { display:block;overflow:auto;position:relative;}

.dateRange input{width:72px;}
.dateRange .limit {text-align: center; width: 15px; line-height:21px; }

/* Pages Form */
.pageForm { display:block; overflow:auto;}
.pageFormContent { display:block; overflow:auto; padding:10px 5px; position:relative;}
.pageFormContent div.unit {display:block; margin:0; padding:5px 0; position:relative;clear:both;}
.pageFormContent p { float:left; display:block; width:420px; height:21px; margin:0; padding:5px 0; position:relative;}
.pageFormContent p.nowrap { height:auto; width: 100%}
.pageFormContent p b { float:left; padding:6px 4px; color:#333333}
.pageFormContent .radioGroup { float:left; display:block; overflow:hidden;}
.pageFormContent label { float:left; width:80px; padding:6px 5px; color:#616161; text-align:right}
.pageFormContent label.radioButton { float:left; width:auto; padding:0 10px 0 0; line-height:21px;}
.pageFormContent .textInput { float:left;}
.pageFormContent select { float:left;}
.pageFormContent .inputInfo { padding:0 5px; line-height:21px;}
.pageFormContent span.unit, .pageFormContent a.unit { padding:0 5px; line-height:21px;}
.pageFormContent span.info{color:#7F7F7F;display:block;line-height:21px;float:left;}

.pageFormContent dl { float:left; display:block; width:380px; height:21px; margin:0; padding:5px 0; position:relative;}
.pageFormContent dt { float:left; width:120px; padding:0 5px; line-height:21px;}
.pageFormContent dd {display:block;line-height:21px;width:220px;float:left;}
.pageFormContent dl.nowrap, .nowrap dl{width:100%; height:auto; clear: both;}
.pageFormContent dl.nowrap dd, .nowrap dd {width: 560px;}

fieldset {display:block; padding:3px 6px; margin:0 0 5px 0; border:1px dotted #cacccf; border-radius:5px}
fieldset legend {padding:5px;  font-weight: bold;  border-radius:3px; color:#5ab629; background-color:#fff}
fieldset legend:hover {background-color: #5ab629; color:#fff}

.formBar { clear:both; padding:0 5px; height:35px; padding-top:5px; border-style:solid; border-width:1px 0 0 0;}
.formBar ul { float:right;}
.formBar li { float:left; margin-left:5px;}
.divider { clear:both; display:block; overflow:hidden; text-indent:-1000px; width:auto; height:1px; padding:0; margin:10px 0; border-style:dotted; border-width:0 0 1px 0;}

/* Pages dialog */
.dialog .pageHeader { border-style:solid; border-width:0;margin:0; background:#fff}
.dialog .pageContent { border-style:solid; border-width:0; background-color:#fff}
.dialog .pageFormContent, .dialog .viewInfo { border-style:solid; border-width: 0;}
.dialog .formBar { border-style:dotted; border-width:1px 0;}

/* combox */
.combox { float:left; margin-right:3px; padding-left:3px;}
.combox select { display:none;}
.combox .select { float:left; background-position:right -25px;}
.combox .select a { float:left; display:block; overflow:hidden; height:23px; padding:0 22px 0 6px; line-height:21px; text-decoration:none; font-size:12px; background-position:100% -50px;}
.comboxop { position: absolute; z-index: 1001; display:none; padding:5px 1px; border-style:solid; border-width:1px; background:#fff; position:absolute; top:22px; left:1px;}
.comboxop a { height:25px; display:block; padding:0 8px; line-height:25px; text-align:left; color:#616161; background:none;}
.comboxop a:hover { background:#e0e0e0; text-decoration:none}
.comboxop .selected { background:#83c442; color:#fff; }
.comboxop .selected:hover { background:#83c442; color:#fff }
.comboxop li { text-align:left; }

/* contentTitle */
h2.contentTitle{margin-bottom:10px; padding:0 10px; line-height:40px; font-size:14px; border-bottom:solid 1px #DDD;}
h3.contentTitle{margin-bottom:5px; line-height:25px; font-size:13px; border-bottom:solid 1px #CCC;clear:both;}
.dialog h2.contentTitle {border:none}

/* rightMenu */
#contextmenu{display:none; position:absolute; z-index:500; left:0; top:0;}
#contextmenu ul{list-style:none; padding:3px; margin:0; background-color:#fff; border:1px solid #bbb; width:150px; border-radius:4px; box-shadow:0 3px 8px rgba(5,28,46,0.4)}
#contextmenu li{display:block; color:#616161; padding:6px; margin:0;  text-align:left; cursor:default; border:1px solid #fff; border-radius:2px;}
#contextmenu li.hover{ border:1px solid #59af1c; color:#fff; text-shadow: 0 1px 0 #3ea01c; font-weight:bold; background-color:#59af1c;
	background-image: -moz-linear-gradient(top, #76c33f, #59af1c);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#76c33f), to(#59af1c));
    background-image: -webkit-linear-gradient(top, #76c33f, #59af1c);
    background-image: -o-linear-gradient(top, #76c33f, #59af1c);
    background-image: linear-gradient(to bottom, #76c33f, #59af1c);}
#contextmenu li.disabled{color:#ddd;}
#contextmenuShadow{display:none; position:absolute; opacity:0; filter:alpha(opacity=0); background-color:#000; z-index:499; }

/* calendar */
#calendar{margin:0; width:214px; z-index:1001; position:absolute; display:block; border:1px solid #bbb; display:none; box-shadow: 0 8px 30px rgba(0,0,0,0.3); border-radius:4px; background:#fff}
#calendar *{margin:0; padding:0; font-size:12px; line-height:18px}
#calendar .main{margin:auto; padding:0; text-align:center; zoom:1; position:relative;}
#calendar .head{ padding:5px 0}
#calendar .head td { text-align:left; width:70px;}
#calendar .head select{width:60px; margin-left:5px; color:#616161}
#calendar .head select option{ color:#616161}
#calendar .head select option:hover{ background:#2b9701}
#calendar .body{clear:both; overflow:hidden;position:relative; background:#ddd; margin:0 5px}
#calendar .foot{ text-align:right;padding:5px; margin-bottom:1px; overflow:hidden}
#calendar .nodate .head, #calendar .nodate .body{display:none;}
#calendar dl{clear:both; margin:auto; overflow:hidden;}
#calendar dl.dayNames, #calendar .okBut{ border-top:1px solid #59af1c; background-color:#5ab629; text-shadow:0 1px 0 #3ea01c;
	background-image: -moz-linear-gradient(top, #81d141, #5ab629);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#81d141), to(#5ab629));
    background-image: -webkit-linear-gradient(top, #81d141, #5ab629);
    background-image: -o-linear-gradient(top, #81d141, #5ab629);
    background-image: linear-gradient(to bottom, #81d141, #5ab629);}
#calendar dt, #calendar dd, #calendar span{width:28px; height:24px; display:block; float:left; overflow:hidden;zoom:1; margin:1px 0 0 1px; background:#fff; line-height:24px; color:#616161}
#calendar dt{font-weight:bold;color:#fff; background:none}
#calendar .days { border-bottom:1px solid #ddd}
#calendar .days dd, #calendar .clearBut{cursor:pointer; background-color:#fff; text-shadow:0 1px 0 #fff;
	background-image: -moz-linear-gradient(top, #fff, #eaeaea);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fff), to(#eaeaea));
    background-image: -webkit-linear-gradient(top, #fff, #eaeaea);
    background-image: -o-linear-gradient(top, #fff, #eaeaea);
    background-image: linear-gradient(to bottom, #fff, #eaeaea);}
#calendar .days dd:hover{ background-color:#cfcfcf; text-shadow:0 1px 0 #fff; color:black;
	background-image: -moz-linear-gradient(top, #eaeaea, #cfcfcf);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#eaeaea), to(#cfcfcf));
    background-image: -webkit-linear-gradient(top, #eaeaea, #cfcfcf);
    background-image: -o-linear-gradient(top, #eaeaea, #cfcfcf);
    background-image: linear-gradient(to bottom, #eaeaea, #cfcfcf);}
#calendar .days dd.other{color:#62ac18}
#calendar .days dd.slt{ background:#81d141; color:#fff; font-weight:bold; text-shadow:0 1px 0 #2b9701; box-shadow:0 0 10px #308900 inset }
#calendar .days dd.disabled {background:#eee; color:#aaa; text-shadow:none; cursor:default}
#calendar .close{width:16px;height:16px; line-height:16px; font-weight:bold; font-size:16px; display:block; float:right; margin-right:5px; cursor:pointer;border:1px solid #CCC;text-align:center; }
#calendar .close:hover{ color:#e23d0b; background:#fef2ee; border-color:#e23d0b}
#calendar .clearBut, #calendar .okBut {width:40px; padding:3px 0; border-radius:2px; border-width:1px; border-style: solid; margin:2px 0 0 5px; box-shadow:0 1px 0 rgba(255,255,255,0.2) inset; cursor:pointer}
#calendar .clearBut{ color:#616161; border-color:#aaa}
#calendar .okBut{color:#fff; border-color:#59af1c}
#calendar .time{border-collapse:collapse;float:left;background-color:#fff;display:none;}
#calendar .time td{border:1px solid #ccc; line-height:16px; vertical-align:center;}
#calendar .time .hh, #calendar .time .mm, #calendar .time .ss{width:18px; height:16px; border: none;color:#616161}
#calendar .time ul {list-style:none}
#calendar .time .up, #calendar .time .down {font-size:8px;height:14px; width:14px; text-align:center; line-height:14px; cursor:pointer;}
#calendar .time .up:hover, #calendar .time .down:hover{ background:#5ab629; color:#fff}
#calendar .tm {text-align:center;}
#calendar .tm .hh, #calendar .tm .mm, #calendar .tm .ss{border:1px solid #cea94a;position:absolute;left:4px;bottom:34px;background-color:#fefccb;width:120px;display:none;box-shadow:0 2px 6px rgba(0,0,0,0.2)}
#calendar .hh .hh, #calendar .mm .mm, #calendar .ss .ss{display:block;}
#calendar .tm .hh li, #calendar .tm .mm li, #calendar .tm .ss li{display:block;float:left;cursor:pointer;width:20px;line-height:21px; color:#a45202}
#calendar .tm .hh li:hover, #calendar .tm .mm li:hover, #calendar .tm .ss li:hover{ background:#cea94a; color:#fff}

/* suggest */
#suggest{position:absolute; z-index:2000; left:0; top:0;}
#suggest ul{list-style:none; padding:1px; margin:0; background-color:#fff; border:1px solid #999; width:150px;}
