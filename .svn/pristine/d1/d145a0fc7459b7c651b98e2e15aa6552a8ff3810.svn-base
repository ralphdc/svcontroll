#sidebar { width:170px; position:absolute; top:0; left:0; z-index:20; overflow:hidden; background:#e0e1e3}
#sidebar_s { display:none; width:24px; position:absolute; top:0; left:0; z-index:21;}
#container { position:absolute; top:57px; left:170px;}
