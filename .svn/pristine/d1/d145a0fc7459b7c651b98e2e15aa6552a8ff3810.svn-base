@charset "utf-8";
::selection { background-color:#83c442; color:#fff}
/* Core Code */
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td { padding:0; margin:0; font-size:12px; line-height:100%; font-family:'Microsoft yahei',Arial, sans-serif;}
h1, h2, h3, h4, h5, h6 { color:#616161}
h1 { line-height:20px; padding-left:10px}
h1.green { color:#5ab629}
h1.line30 { line-height:30px}
h1.line40 { line-height:40px}
h6 { line-height:16px; font-size:16px; font-weight:bold; color:#5ab629; text-align:center}

/*pagerForm*/
#pagerForm { display:none}

/* Remember to define focus styles! */
:focus { outline: 0;}
body { width:100%; height:100%; /* text-align:center; */ color:black; min-width:800px}
ul, ol { list-style:none;}

/* Tables still need 'cellspacing="0"' in the markup. */
table { border-collapse:separate; border-spacing:0;}
caption, th, td { font-weight:normal;}

/* Remove possible quote marks (") from <q>, <blockquote>. */
blockquote:before, blockquote:after,
q:before, q:after { content: "";}
blockquote, q { quotes: "" "";}

img { border:none;}
a { color:#616161; text-decoration:none;}
a:hover {text-decoration:underline;}

/* Panel */
.panel { display:block; background-position:0 100%; background-repeat:repeat-x;}
.panel .panelHeader { display:block; height:37px; padding-left:5px; background-position:0 0;}
.panel .panelHeaderContent { display:block; height:37px; padding-right:5px; background-position:100% -50px; position:relative;}
.panel .panelHeaderContent h1 { display:block; overflow:hidden; height:37px; padding:0 5px;line-height:37px; background-position:0 -100px; background-repeat:repeat-x;}
.panel .panelContent { display:block; overflow:auto; padding:5px 5px 1px 5px; border-style:solid; border-width:0 1px;}
.panel .panelFooter { display:block; overflow:hidden; height:5px; padding-left:5px; background-position:0 -150px;}
.panel .panelFooterContent { display:block; overflow:hidden; height:5px; padding-right:5px; background-position:100% -200px;}

.panel .collapsable, .panel .expandable { display:block; overflow:hidden; width:20px; height:21px; text-indent:-1000px; position:absolute; top:4px; right:4px;}
.panel .collapsable:hover { background-position:0 -50px;}
.panel .expandable { background-position:0 -100px;}
.panel .expandable:hover { background-position:0 -150px;}

/* Tabs */
.tabs { background-position:0 100%; background-repeat:repeat-x; border:1px solid #cfd1d3; border-top:0; margin:5px}
.tabs .tabsHeader { display:block; overflow:hidden; background-position:0 0; background-repeat:repeat-x;}
.tabs .tabsHeaderContent { display:block; overflow:hidden; background:none}
.tabs .tabsHeader ul { display:block; background-position:0 -100px; background-repeat:repeat-x;}
.tabs .tabsHeader li { float:left; display:block; border-right:1px solid #d8d9da; background-position:0 -250px; background-repeat:repeat-x; cursor:pointer;}
.tabs .tabsHeader li a { float:left; display:block; padding-left:15px; background-position:0 -150px;}
.tabs .tabsHeader li a:hover { text-decoration:none;}
.tabs .tabsHeader li span { float:left; display:block; overflow:hidden; padding:14px 15px 14px 0; background-position:right -200px; cursor:pointer;}
.tabs .tabsHeader li.hover { background-position:0 -400px; background-repeat:repeat-x;}
.tabs .tabsHeader li.hover a { background-position:0 -300px;}
.tabs .tabsHeader li.hover span { background-position:100% -350px; color:#5ab629}
.tabs .tabsHeader li.selected { background-position:0 -550px;}
.tabs .tabsHeader li.selected a { background-position:0 -450px;}
.tabs .tabsHeader li.selected span { font-weight:bold; background-position:100% -500px; color:#5ab629; text-shadow:none}
.tabs .tabsContent { display:block; overflow:auto; }
.tabs .tabsFooter { display:none; overflow:hidden; height:5px; background-position:0 -600px;}
.tabs .tabsFooterContent { display:block; overflow:hidden; height:5px; background-position:100% -650px;}


.tabsPage .tabsPageHeader { display:block; height:32px; background-position:0 -10000px; position:relative;}
.tabsPage .tabsPageHeaderContent .navTab-tab li a span { font-size:14px;}
.tabsPage .tabsPageHeaderContent { display:block; overflow:hidden; height:32px; margin-right:19px; position:relative;}
.tabsPage .tabsPageHeaderMargin { margin:0 4px 0 2px;}
.tabsPage .tabsPageHeader ul { display:block; width:10000px; height:32px; z-index:1; position:absolute;}
.tabsPage .tabsPageHeader li { float:left; display:block; height:32px; margin-left:2px; background-position:0 -100px; background-repeat:repeat-x; position:relative; cursor:pointer;}
.tabsPage .tabsPageHeader li a { float:left; display:block; overflow:hidden; height:32px; padding-left:25px; line-height:32px; background-position:0 0; }
.tabsPage .tabsPageHeader li a:hover { text-decoration:none;}
.tabsPage .tabsPageHeader li span { float:left; display:block; overflow:hidden; min-width:50px; max-width:150px; height:32px; padding-right:30px; line-height:32px; background-position:100% -50px; cursor:pointer;}

.tabsPage .tabsPageHeader li.hover { background-position:0 -250px; background-repeat:repeat-x;}
.tabsPage .tabsPageHeader li.hover a { background-position:0 -150px;}
.tabsPage .tabsPageHeader li.hover span { background-position:100% -200px;}

.tabsPage .tabsPageHeader li.selected { background-position:0 -400px; background-repeat:repeat-x;}
.tabsPage .tabsPageHeader li.selected a { font-weight:bold; background-position:0 -300px;}
.tabsPage .tabsPageHeader li.selected span { background-position:100% -350px;}

.tabsPage .tabsPageHeader li .close { display:block; overflow:hidden; width:14px; height:15px; padding:0; text-indent:-1000px; position:absolute; top:5px; right:8px;}
.tabsPage .tabsPageHeader li .close:hover, .tabsPage .tabsPageHeader li.selected .close:hover { background-position:0 -60px;}
.tabsPage .tabsPageHeader li.selected .close { background-position:0 -30px;}

.tabsPage .tabsPageHeader li.main span { padding:0 8px 0 3px;}
.tabsPage .tabsPageHeader li .home_icon,
.tabsPage .tabsPageHeader li.main .home_icon { width:auto; padding:0 0 0 17px; background-position:0 4px;}
.tabsPage .tabsPageHeader li.selected.main .home_icon { background-position:0 -134px;}

.tabsPage .tabsMove { height:25px; position:absolute; top:0; right:0; z-index:2;}
.tabsPage .tabsLeft, .tabsPage .tabsRight, .tabsPage .tabsMore { display:block; overflow:hidden; width:50px; height:32px; text-indent:-1000px; position:absolute; z-index:2;}
.tabsPage .tabsLeft { background-position:0 0; top:0px; left:-2px; cursor:pointer;}
.tabsPage .tabsLeftHover { background-position:0 -50px;}
.tabsPage .tabsLeftDisabled { background-position:0 -100px; cursor:default;}
.tabsPage .tabsRight { background-position:0 -150px; top:0px; right:19px; cursor:pointer;}
.tabsPage .tabsRightHover { background-position:0 -200px;}
.tabsPage .tabsRightDisabled { background-position:0 -250px; cursor:default;}
.tabsPage .tabsMore { background-position:0 -300px; top:0px; width:25px; right:-3px; cursor:pointer;}
.tabsPage .tabsMoreHover { background-position:0 -350px;}
.tabsPage .tabsMoreList { display:none; overflow-x:hidden; overflow-y:auto; width:170px; max-height:380px; padding:3px; border-style:solid; border-width:1px; position:absolute; top:33px; right:0; z-index:3; border-radius:4px; box-shadow:0 3px 8px rgba(5,28,46,0.4); color:#616161}
.tabsPage .tabsMoreList li { display:block; overflow:hidden; }
.tabsPage .tabsMoreList li a { display:block; width:148px; padding:6px 10px; border:solid 1px #FFF; border-radius:2px; white-space:nowrap; color:#616161 }
.tabsPage .tabsMoreList li a:hover {text-decoration:none; border:1px solid #59af1c; color:#fff; text-shadow: 0 1px 0 #3ea01c; font-weight:bold; background-color:#59af1c;
	background-image: -moz-linear-gradient(top, #76c33f, #59af1c);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#76c33f), to(#59af1c));
    background-image: -webkit-linear-gradient(top, #76c33f, #59af1c);
    background-image: -o-linear-gradient(top, #76c33f, #59af1c);
    background-image: linear-gradient(to bottom, #76c33f, #59af1c);}
.tabsPage .tabsMoreList li.selected a { font-weight:bold; border-color:#b8b9ba; background:#dadcde;}
.tabsPage .tabsMoreList li.selected a:hover { text-shadow:none; color:#616161;}
.tabsPage .tabsPageContent { display:block; overflow:hidden; border-style:solid; border-width:0 0 1px 1px; position:relative;}

/* Alert */
/*.alert { overflow:hidden; position:absolute; z-index:101; width:300px; top:0}*/
.alert { overflow:hidden; z-index:1011; display:block; width:300px; margin-left:-150px; position:absolute; top:0; left:50%; box-shadow:0 5px 20px rgba(0,0,0,0.3); border-radius: 0 0 4px 4px}
.alert .alertContent { display:block; overflow:hidden; padding:5px 5px 1px 5px; border-style:solid; border-width:0 1px 1px 1px; border-radius:0 0 4px 4px}
.alert .alertInner { display:block; padding:0 9px 9px 9px; text-align:left; border-style:solid; border-width:1px;}
.alert .alertInner .msg { padding:10px; max-height: 200px; overflow: auto; line-height: 1.3em; text-shadow:0 0 3px #fff; font-weight:bold}
.alert h1 { display:block; overflow:hidden; height:30px; margin-bottom:5px; padding:0 0 0 25px; line-height:30px; border-style:solid; border-width:0 0 1px 0; line-height:30px; color:#000; text-shadow:0 0 3px #fff}
.alert .error h1 { background-position:2px -45px;}
.alert .info h1,
.alert .warn h1 { background-position:2px 5px;}
.alert .correct h1 { background-position:2px -95px;}
.alert .confirm h1 { background-position:2px 5px;}
.alert p { margin:10px;}

.alert .toolBar { display:block; overflow:hidden; padding:5px 0; text-align:right;}
.alert .toolBar ul { float:right;}
.alert .toolBar li { float:left;}
.alert .toolBar .button, .alert .toolBar .buttonActive { margin-left:5px;}

.alert .alertFooter_c { display:block; overflow:hidden; height:5px;}
.alert .alertFooter { display:none; padding-left:5px; background-position:0 0; border-radius:2px}
.alert .alertFooter_r { padding-right:5px; background-position:100% -50px;}
.alert .alertFooter_c { padding:0; background-position:0 -100px; background-repeat:repeat-x;}

/* Dialog */
.dialog { display:block; text-align:left; position:absolute; z-index:42; box-shadow: 0 10px 40px rgba(0,0,0,0.4); border-radius: 4px;}
.dialog .dialogHeader, .dialog .dialogHeader_r, .dialog .dialogHeader_c { display:block; overflow:hidden; height:34px;}
.dialog .dialogHeader .close, .dialog .dialogHeader .maximize, .dialog .dialogHeader .restore, .dialog .dialogHeader .minimize { display:block; overflow:hidden; text-indent:-1000px; width:30px; height:22px; position:absolute; top:1px; border-left:1px solid #60a51f }
.dialog .dialogHeader h1 { display:block; overflow:hidden; height:34px; padding:0 5px 0 25px; line-height:34px; background-position:0 -448px;}

.dialog .dialogHeader { padding-left:5px; background-position:0 0; position:relative; cursor:move;}
.dialog .dialogHeader_r { padding-right:5px; background-position:100% -50px;}
.dialog .dialogHeader_c { padding:0; background-position:0 -100px; background-repeat:repeat-x;}

.dialog .dialogHeader .close { background-position:0 0; right:1px;}
.dialog .dialogHeader .close:hover { background-position:0 -50px;}
.dialog .dialogHeader .maximize { background-position:0 -100px; right:32px;}
.dialog .dialogHeader .maximize:hover { background-position:0 -150px;}
.dialog .dialogHeader .restore { display:none; background-position:0 -200px; right:32px;}
.dialog .dialogHeader .restore:hover { background-position:0 -250px;}
.dialog .dialogHeader .minimize { background-position:0 -300px; right:63px;}
.dialog .dialogHeader .minimi  ze:hover { background-position:0 -350px;}
.dialog .dialogContent { display:block; overflow:hidden; padding:5px 5px 1px 5px; border-style:solid; border-width:0 1px;}
.dialog .panelFooter_r, .dialog .dialogFooter_c { display:block; overflow:hidden; height:5px;}
.dialog .dialogFooter { padding-left:5px; background-position:0 -150px;}
.dialog .dialogFooter_r { padding-right:5px; background-position:100% -200px;}
.dialog .dialogFooter_c { padding:0; background-position:0 -250px; background-repeat:repeat-x;}

.dialogProxy { opacity:0.8; filter:alpha(opacity=80);}

.dialog .resizable_f_r { width:11px; height:11px; background-position:0 -400px;}

/* Dialog Resizable */
.resizable { display:none; overflow:hidden; border-style:dashed; border-width:1px; opacity:0.5; filter:alpha(opacity=50); position:absolute; top:0; left:0; z-index:100;}
.resizable_h_l, .resizable_h_r, .resizable_h_c,
.resizable_c_l, .resizable_c_r,
.resizable_f_l, .resizable_f_r, .resizable_f_c { display:block; overflow:hidden; width:6px; height:6px; position:absolute;}
.resizable_h_l { cursor:nw-resize; top:0; left:0; z-index:2;}
.resizable_h_r { cursor:ne-resize; top:0; right:0; z-index:2;}
.resizable_h_c { width:100%; cursor:n-resize; top:0; left:0; z-index:1;}
.resizable_c_l { cursor:w-resize; top:0; left:0; z-index:1;}
.resizable_c_r { cursor:e-resize; top:0; right:0; z-index:1;}
.resizable_f_l { cursor:sw-resize; bottom:0; left:0; z-index:2;}
.resizable_f_r { cursor:se-resize; bottom:0; right:0; z-index:2;}
.resizable_f_c { width:100%; cursor:s-resize; bottom:0; left:0; z-index:1;}

/* Shadow */
.shadow { display:none; overflow:hidden; position:absolute; z-index:41; opacity:0}
.shadow .shadow_h, .shadow .shadow_h_l, .shadow .shadow_h_r, .shadow .shadow_h_c,
.shadow .shadow_f, .shadow .shadow_f_l, .shadow .shadow_f_r, .shadow .shadow_f_c { display:block; overflow:hidden; height:11px;}

.shadow .shadow_h, .shadow .shadow_c, .shadow .shadow_f { position:relative;}
.shadow .shadow_h_l, .shadow .shadow_c_l, .shadow .shadow_f_l { width:11px; position:absolute; top:0; left:0;}
.shadow .shadow_h_r, .shadow .shadow_c_r, .shadow .shadow_f_r { width:11px; position:absolute; top:0; right:0;}
.shadow .shadow_h_c, .shadow .shadow_c_c, .shadow .shadow_f_c { margin:0 11px;}

.shadow .shadow_c, .shadow .shadow_c_l, .shadow .shadow_c_r, .shadow .shadow_c_c { display:block; overflow:hidden; height:100%;}

/* Tree */
.tree li { clear:both; display:block; line-height:22px; cursor:pointer;}
.tree div, .tree a, .tree span { display:inherit; height:28px; line-height:28px;}
.tree div { display:block; overflow:hidden;}
.tree div div { float:left; display:block; overflow:hidden; width:28px; height:28px; border:none; background-position:0 -100px;}
.tree a, .tree a:hover { text-decoration:none;}

.tree .collapsable { background-position:0 -300px;}
.tree .first_collapsable { background-position:0 -250px;}
.tree .last_collapsable { background-position:0 -350px;}
.tree .expandable { background-position:0 -100px;}
.tree .first_expandable { background-position:0 -50px;}
.tree .last_expandable { background-position:0 -150px;}
.tree .end_expandable { background-position:0 0;}
.tree .end_collapsable { background-position:0 -200px;}

.tree.leftmenu .collapsable { float:right;}
.tree.leftmenu .first_collapsable { float:right;}
.tree.leftmenu .last_collapsable { float:right;}
.tree.leftmenu .expandable { float:right;}
.tree.leftmenu .first_expandable { float:right;}
.tree.leftmenu .last_expandable { float:right;}
.tree.leftmenu .end_expandable { float:right;}
.tree.leftmenu .end_collapsable { float:right;}

.tree .indent { background:none;}
.tree .line { background-position:0 -400px;}
.tree .node { background-position:0 -450px;}
.tree .last .node { background-position:0 -500px;}

.tree .folder_expandable { background-position:0 0;}
.tree .folder_collapsable { background-position:0 -50px;}
.tree .file { background-position:0 -100px;}
.tree.leftmenu .folder_expandable { display:none}
.tree.leftmenu .folder_collapsable { display:none}
.tree.leftmenu .file { width:18px; background-position:0 -92px; background-image:url(../img/icon.png)}
.tree.leftmenu .node { display:none}
.tree.leftmenu li { margin:1px 0; background:#f5f5f5}
.tree.leftmenu li ul, .tree.leftmenu li ul li { background:#fff}
.tree.leftmenu li.nonesub .file { display:none}
.tree.leftmenu li.nonesub .tree_menu { padding-left:20px}

.tree .unchecked { background-position:0 0;}
.tree .hover .unchecked { background-position:0 -50px;}
.tree .checked { background-position:0 -100px;}
.tree .hover .checked { background-position:0 -150px;}
.tree .indeterminate { background-position:0 -200px;}
.tree .hover .indeterminate { background-position:0 -250px;}

/* Accordion */
.accordion { display:block; border-style:solid; border-width:0;}
.accordion .accordionHeader { display:block; overflow:hidden; background-repeat:repeat-x; cursor:pointer;}
.accordion .accordionHeader h2 { display:block; overflow:hidden; padding:0 25px 0 5px; height:41px; line-height:40px;}
.accordion .accordionHeader h2 span { float:left; display:block; overflow:hidden; text-indent:-1000px; width:20px; height:25px;}
.accordion .accordionContent { display:block; overflow:auto; border-style:solid; border-width:0 0 1px 0; background:#e9eaeb}

.accordion .accordionHeader.hover { background-position:0 -41px;}
.accordion .accordionHeader h2 { background-repeat:no-repeat; background-position:135px -115px;}
.accordion .accordionHeader.hover h2 { background-position:135px -185px; color:#5ab629}
.accordion .accordionHeader .collapsable { background-position:135px -255px;}
.accordion .accordionHeader.hover .collapsable { background-position:135px -325px;}
.accordion .accordionHeader h2 span { background-position:0 -150px;}
.accordion .accordionHeader h2 div { font-size:14px; padding: 14px 0 14px 43px; background-position: 6px center; background-repeat: no-repeat;}


#leftshadow { width:10px; background:url(../img/accordion/leftshadow.png) repeat-y; position:absolute; top:-6px; left:160px; z-index:21}

/* Grid */
.panel .grid { border-style:solid; border-width:0 1px;}
.panel .panelBar { border-width:1px;}

.panelBar { display:block; overflow:hidden; height:37px; background-repeat:repeat-x; }

.panelBar ul { padding:5px 4px;}
.panelBar li { float:left; display:block; overflow:hidden; height:29px; padding:0 0 0 6px;}
.panelBar li { background-position:0 1000px;}
.panelBar li a { background-position:100% 1000px;}
.panelBar li.hover { background-position:0 -90px;}
.panelBar li.hover a { background-position:100% -170px;}
.panelBar li:active { background-position:0 -50px;}
.panelBar li:active a { background-position:100% -130px;}

.panelBar .toolBar li, .panelBar .toolBar li.hover { padding:0 0 0 10px;}
.panelBar .toolBar a, .panelBar .toolBar li.hover a { float:left; display:block; overflow:hidden; padding:0 10px 0 0; text-decoration:none; }
.panelBar .toolBar span, .panelBar .toolBar li.hover span { color:#616161; text-shadow:0 1px 0 #fff; font-weight:bold; float:left; display:block; overflow:hidden; height:22px; padding:8px 5px 0 26px; cursor:pointer;}

.panelBar .toolBar a.add span { background-position:5px -695px;}
.panelBar .toolBar a.delete span { background-position:5px -745px;}
.panelBar .toolBar a.edit span { background-position:5px -795px;}
.panelBar .toolBar a.lock span { background-position:5px -896px;}
.panelBar .toolBar a.unlock span { background-position:5px -947px;}
.panelBar .toolBar a.onekey span { background-position:-168px -795px;}
.panelBar .toolBar a.selectedkey span { background-position:-318px -795px;}
.panelBar .toolBar a.icon span { background-position:5px -845px;}

.panelBar .toolBar li.line { display:block; overflow:hidden; width:12px; padding:0; text-indent:-1000px; background-position:5px -200px;}

.panelBar .pages { float:left; overflow:hidden; height:21px; padding:8px 10px; color:#616161; text-shadow:0 1px 0 #fff}
.panelBar .pages span { float:left; line-height:21px;}
.panelBar .pages select { float:left; margin:0 3px; font-size:12px;}

.pagination { float:right; padding-left:7px; }
.pagination li { height:29px; padding:0 0 0 10px; margin-left:1px }
.pagination a, .pagination li.hover a, .pagination li span { height:29px; float:left; display:block; padding:0 10px 0 0; text-decoration:none; line-height:27px; background-position:100% -150px; text-shadow:0 1px 0 #fff}
.pagination li.selected a{color:#fff;}
.pagination span, .pagination li.hover span { float:left; display:block; height:29px; line-height:27px; cursor:pointer;}

.pagination li a { background-position:right 1000px;}
.pagination li.hover { background-position:0 -90px;}
.pagination li.hover a { background-position:right -170px;}
.pagination li:active { background-position:0 -50px;}
.pagination li:active a { background-position:right -130px;}

.pagination li.selected, .pagination li.selected a { height:26px; background:#5ab629; text-shadow:0 1px 0 #2c7200;
	background-image: -moz-linear-gradient(top, #5ab629, #83c442);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#5ab629), to(#83c442));
    background-image: -webkit-linear-gradient(top, #5ab629, #83c442);
    background-image: -o-linear-gradient(top, #5ab629, #83c442);
    background-image: linear-gradient(to bottom, #5ab629, #83c442);}
.pagination li.selected { border-radius:3px}

.pagination li .first span, .panelBar li .previous span { padding:0 0 0 10px;}
.pagination li .next span, .panelBar li .last span { padding:0 10px 0 0;}
.pagination li .first span { background-position:0 -242px;}
.pagination li .previous span { background-position:0 -292px;}
.pagination li .next span { background-position:100% -342px;}
.pagination li .last span { background-position:100% -392px;}
.pagination li .last { margin-right:5px;}

.pagination li.disabled { background:none;}
.pagination li.disabled span, .grid .pagination li.disabled a { background-position:0 100px; cursor:default;}
.pagination li.disabled span span { color:#666;}
.pagination li.disabled .first span { background-position:0 -442px;}
.pagination li.disabled .previous span { background-position:0 -492px;}
.pagination li.disabled .next span { background-position:100% -542px;}
.pagination li.disabled .last span { background-position:100% -592px;}
.pagination li.disabled .last { margin-right:5px;}

.pagination li.jumpto { padding:2px 2px 0 7px; background-position:0 -200px;}
.pagination li.jumpto .textInput { float:left; width:30px; padding:1px; border-color:#acaeaf;}
.pagination li.jumpto .goto { float:left; display:block; overflow:hidden; width:16px; height:21px; border:0; text-indent:-1000px; background-position:0 -650px; cursor:pointer;}

.grid { display:block; overflow:hidden; width:100%; border-width:0 1px; position:relative; color:#616161;}
.grid .gridHeader { display:block; overflow:hidden; width:auto; border-top:1px solid #d8d8d8}


.grid .gridScroller { display:block; overflow:auto; position:relative;}
.grid .gridTbody input[type="checkbox"], .grid .gridThead input[type="checkbox"], table.list input[type="checkbox"] { margin-top:8px}

.grid table { border:0; border-collapse:collapse;table-layout:fixed;}
.grid .gridHeader th { padding:0 3px; border-style:solid; border-width:0 1px 1px 0; vertical-align:top; white-space:nowrap; line-height:30px; cursor:default;}
.grid .gridHeader th.hover, .grid .gridHeader th.thSelected { background:#fbfbfb;}
