<div class="pageContent">
	<form method="post" action="__URL__/edit/id/{$element['id']}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone)" novalidate="novalidate">
		<div class="pageFormContent" layouth="58">
			<p style="clear:both;width: 100%;height: auto;">
				<label>产品名称：</label>
				<span class="info">{$_COOKIE['productName']}</span>
			</p>
			<p>
				<label>元素名称：</label>
				<input type="text" name="elementName" id="elementName" class="textInput" value="{$element['elementName']}" class="textInput" />
			</p>
			<p>
				<label>元素编码：</label>
				<input type="text" name="elementCode" id="elementName" class="textInput" value="{$element['elementCode']}" />
			</p>
			
			<p>
				<label>备注信息：</label>
				<textarea name="remark" class="editor textInput" cols="40" rows="3" maxlength="100">{$element['remark']}</textarea>
			</p>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">确定</button></div></div></li>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div></li>
			</ul>
		</div>
	</form>
</div>


